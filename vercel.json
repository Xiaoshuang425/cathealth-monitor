{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/docs/dashboard.html"
    },
    {
      "src": "/dashboard",
      "dest": "/docs/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ]
}
